
<h1> RESULTS </h1>

<% if @result[0].is_a? Event %>
  <% @result.each do |event| %>
    <%= event.name %>
  <%end%>
<% elsif  @result[0].is_a? Venue %>
  <% @result.each do |venue| %>
      <%= venue.name %>
  <%end%>
<% elsif  @result[0].is_a? Comedian %>
  <% @result.each do |comedian| %>
      <a href=""<%= comedian.first_name %>
  <%end%>
<% else %>
<% "Did now find any results" %>

<% end%> 
<%= @result %> 


