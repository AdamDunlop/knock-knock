  <div class="container jumbo-event">
    <div class="container">
      <div class="venue-jumbo">
      </div>
    </div>    
  </div>

<div class="container event-show">
  <div class="row">
    <div class="col-md-7 info-item">
      <h1><%= "#{@venue.name}"%></h1>
      <p><%= "#{@venue.location} <br> #{@venue.phone_number} <br> #{@venue.email}" %></p>
       <button type="button" class="btn btn-success btn-sm" data-toggle="modal" data-target="#request">
        Request Booking
      </button>
    </div> 
    <div class="col-md-1">
    </div>
    <div class="col-md-4">
      <img class="comedian-image venue-image2" src="<%= @venue.picture_url %>" height="300" width="300">
    </div>
  </div>
<hr>
  <div class="venue-map">
    <div id="mymap"></div>
    <input id="address" type="hidden" value="<%= @venue.location %>">  
  </div>


  <div class="container">
  <h3 style="text-align:center;"><em>Other events at the <%= "#{@venue.name}" %></em></h3>
  <hr> 
  </div>  

  
  <div class="row">
      <% @events.each do |event| %>
      <div class="col-md-4 other-event">
          <h3><%= "#{event.name}"%></h3>        
          <a href="/events/<%= event.id%>">  
          <div class="thumbnail">
            <img class="home-comedians" src="<%= event.picture_url %>" alt="...">
          </div>
          </a>
 
       </div> 
      <%end%>  
  </div>
  </div>
  </div>
  <hr>

   <div class="modal fade" id="request" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="myModalLabel">Bookings</h4>
            </div>
            <div class="modal-body">
              <form method="post" action="/redirect">
              <div class="form-group">
                <label for="exampleInputEmail1">Request Booking</label>
                <input type="text" name="email" class="form-control content" id="exampleInputEmail1" placeholder="Hello, can I please do some comedy at your club? Thanks">
              </div>
              
                
                 <input type="submit" class="btn btn-primary">
              </form>

            </div>
            <div class="modal-footer">
          
            </div>
          </div>
        </div>
      </div>