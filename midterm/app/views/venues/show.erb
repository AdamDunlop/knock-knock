
<div class="container jumbo-venue">

  <div class="container">
    <div class="venue-jumbo">
      <h1>Venue</h1>
      <p><%= "#{@venue.name}"%><%= " | Location: #{@venue.location}"%></a>
      </p>
      </div>
    <img class="venue-image" src="http://www.thecomedymix.com/wp-content/uploads/2015/05/Brian-Scolaro-210x170.jpg" height="180" width="180">
  </div>
</div>

<div class="container venue-show">
  <div class="white-container">
    <div class="venue-map">

    </div>
    <h2>Upcoming events</h2>
    <hr>
    <p><% @events.each do |event| %>
      <div class="row venue-event"><a href="www.google.com">
        <div class="col-md-2 first-block">
         <img class="comedian-image2" src="http://www.thecomedymix.com/wp-content/uploads/2015/05/Brian-Scolaro-210x170.jpg" height="120" width="120">
       </div>
       <div class="col-md-4">
        <h4 class="single-event"><p>Event: <a href="/events/<%= event.id %>"><%= "#{event.name}" %></a><%= "| Date: #{event.date} | Location: #{event.venue.name} | Comedian:"%><a href="/comedians/<%= event.comedian.id %>"><%=" #{event.comedian.first_name} #{event.comedian.last_name}"%></a></p></h4>
      </div>
      <div class="col-md-6">


      </div>
    </div>
  </a>
  <% end %>
</p>
</div>
</div>