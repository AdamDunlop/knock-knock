<h1>Venues</h1>
    <div class="venue-map">
      <div id="map" style="width: 600px; height: 400px;"></div>
      </div>
      
<p><% @venues.each do |venue| %>
      <div class="col-md-6">
        <h4 class="single-event"><p>Venue: <a href="/venues/<%= venue.id %>"><%= "#{venue.name}" %></a><%= " | Location: #{venue.location} "%></h4>
      </div>
  <% end %></p>




